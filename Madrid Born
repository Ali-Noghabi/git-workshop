using System;

namespace HelloWorld
{
    class Program
    {
        static void Main(string[] args)
        {
            var x = "using System;\n\nnamespace HelloWorld\n{{\n    class Program\n    {{\n        static void Main(string[] args)\n        {{\n            Console.WriteLine(\"Hello World!\");    \n        }}\n    }}\n}}";
            Console.WriteLine(x);
        }
    }
}